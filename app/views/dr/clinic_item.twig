<div class="list-item" data-id="{{ clinic_id }}">
	<div class="name">{{ name }}</div>
	<div class="loc">{{ location }}</div>
	<div class="sched">
		<ul>
		{% for sched in schedule %}
			<li>{{ sched.day ~ ' ' ~ sched.opening ~ ' - ' ~ sched.closing }}</li>
		{% endfor %}
		</ul>
	</div>
	<div class="actions">
		<a class="edit" href="#"><i class="fas fa-fw fa-edit"></i> Edit</a>
		<a class="delete" href="#"><i class="fas fa-fw fa-trash"></i> Delete</a>
	</div>
</div>